logfile q4md-forcefieldtools.log

# Web site: https://upjv.q4md-forcefieldtools.org/REDServer-Development/Documentation/AmberTools12.pdf#alias
alias q quit
alias e edit
alias c charge

# Web site: https://upjv.q4md-forcefieldtools.org/REDServer-Development/Documentation/AmberTools12.pdf#verbosity
verbosity 2

# Web site: https://upjv.q4md-forcefieldtools.org/REDServer-Development/Documentation/AmberTools12.pdf#addAtomTypes
addAtomTypes {
         { "C"   "C"   "sp2" }
         { "CA"  "C"   "sp2" }
         { "CM"  "C"   "sp2" }
         { "CT"  "C"   "sp3" }
         { "H"   "H"   "sp3" }
         { "H1"  "H"   "sp3" }
         { "H4"  "H"   "sp3" }
         { "HA"  "H"   "sp3" }
         { "HC"  "H"   "sp3" }
         { "N2"  "N"   "sp2" }
         { "NA"  "N"   "sp2" }
         { "NC"  "N"   "sp2" }
         { "O"   "O"   "sp2" }
         { "OS"  "O"   "sp3" }
         { "S"   "S"   "sp3" }
}

